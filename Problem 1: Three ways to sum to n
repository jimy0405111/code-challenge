var sum_to_n_a = function(n) {
    // Implementation 1: Mathematical formula
    return (n * (n + 1)) / 2;
};

var sum_to_n_b = function(n) {
    // Implementation 2: Iterative loop
    let sum = 0;
    for (let i = 1; i <= n; i++) {
        sum += i;
    }
    return sum;
};

var sum_to_n_c = function(n) {
    // Implementation 3: Recursive function
    if (n <= 1) return n;
    return n + sum_to_n_c(n - 1);
};
